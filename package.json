{
  "name": "retirement-simulator",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev --turbopack",
    "dev:port": "next dev --turbopack --port 3000",
    "build": "next build --turbopack",
    "start": "next start",
    "stop": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'Port 3000 is already free'",
    "restart": "npm run stop && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@openai/codex": "^0.55.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sparticuz/chromium": "^141.0.0",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "handlebars": "^4.7.8",
    "intl-messageformat": "^10.7.18",
    "lucide-react": "^0.552.0",
    "nanoid": "^5.1.6",
    "next": "16.0.1",
    "next-intl": "^4.4.0",
    "puppeteer-core": "^24.29.0",
    "react": "19.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.3.0",
    "tailwind-merge": "^3.3.1",
    "vega": "^6.2.0",
    "vega-lite": "^6.4.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.0.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.15.1"
}
